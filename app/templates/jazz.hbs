{{outlet}}

<section class="section-container artists">
<img class="large-icon" src="/img/music-tiles/folk-grey.svg" alt="">
  <ul>
    {{#each artist in filteredArtists}}
    <li>
      <a class="learn-more" href="{{artist.urls.mb_url}}">
        <p><span class="blue-link">Learn more about: </span>{{artist.name}}</p>
      </a>
    </li>
    {{/each}}
  </ul>
</section>